<?xml version="1.0" encoding="UTF-8"?>

<sql-statement xmlns="http://xml.mxi.com/xsd/common/dataset/sql/1.0">
   <name>GetExceptLrpLocCapacity</name>
   <description>This query retrieves an LRP Locations by Plan key</description>
   <sql>
      SELECT
         *
      FROM
         lrp_loc_cap_except
      WHERE
      lrp_loc_cap_except.lrp_db_id = :aLRPDbId   AND
      lrp_loc_cap_except.lrp_id    = :aLRPId    AND
      lrp_loc_cap_except.loc_db_id = :aLocDbId AND
      lrp_loc_cap_except.loc_id    = :aLocId  AND
      lrp_loc_cap_except.capacity_pattern_db_id = :aCapacityDbIdId  AND
      lrp_loc_cap_except.capacity_pattern_id    = :aCapacityId AND
      lrp_loc_cap_except.capacity_except_id   = :aCapacityExceptId
     AND
      lrp_loc_cap_except.rstat_cd   = 0
    </sql>
    <arguments>
      <argument name="aLRPDbId" type="INTEGER" default="-1" />
      <argument name="aLRPId"   type="INTEGER" default="-1" />
      <argument name="aLocDbId" type="INTEGER" default="-1" />
      <argument name="aLocId"   type="INTEGER" default="-1" />
      <argument name="aCapacityDbIdId" type="INTEGER" default="-1" />
      <argument name="aCapacityId" type="INTEGER" default="-1" />
      <argument name="aCapacityExceptId" type="INTEGER" default="-1" />
   </arguments>
</sql-statement>